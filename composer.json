{
    "name": "steffenbrem/json-api-bundle",
    "description": "Integration of JSON API with Symfony2 using JMS Serializer (FOSRestBundle).",
    "keywords": [
        "rest",
        "jsonapi",
        "json-api"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Steffen Brem",
            "email": "steffenbrem@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "symfony/yaml": "dev-master",
        "symfony/property-access": "dev-master",
        "jms/serializer-bundle": "dev-master",
        "pagerfanta/pagerfanta": "dev-master",
        "willdurand/hateoas": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Mango\\Bundle\\JsonApiBundle": ""
        }
    },
    "target-dir": "Mango/Bundle/JsonApiBundle",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
